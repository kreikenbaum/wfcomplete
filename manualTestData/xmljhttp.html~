<html>
  <head>
    <script>
      function loadXMLDoc() {
      if (window.XMLHttpRequest) {// code for
      IE7+, Firefox, Chrome, Opera, Safari xmlhttp=new
      XMLHttpRequest(); } else {// code for IE6, IE5 xmlhttp=new
      ActiveXObject("Microsoft.XMLHTTP"); }
      xmlhttp.onreadystatechange=function() { if
      (xmlhttp.readyState==4 && xmlhttp.status==200) {
      document.getElementById("myDiv").innerHTML=xmlhttp.responseText;
      } }
      xmlhttp.open("GET","http://http://127.0.0.1:8082/post/mxwnnnqaflufnqwlekfd",true);
      xmlhttp.send(); }
    </script>
  </head>
  <body onload= loadXMLDoc>
    <p>This is a body</p>
  </body>
</html>
